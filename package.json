{
  "name": "take-home-user-form",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently -n vite,api \"npm run vite:dev:wait\" \"npm run api:dev\"",
    "build": "npm run vite:build && npm run api:build",
    "start": "node ./.local/express/dist/api.js",
    "vite:build": "vite build --emptyOutDir",
    "vite:dev": "vite",
    "vite:dev:wait": "wait-on http://localhost:9001/api/v1/health && vite",
    "api:build": "node esbuild.mjs",
    "api:build:watch": "npm run api:build -- --watch",
    "api:dev": "concurrently -n build,serve \"rimraf ./.local/express/dist && npm run api:build:watch\" \"wait-on ./.local/express/dist/api.js && nodemon --inspect=8229 --legacy-watch --watch ./.local/express/dist ./.local/express/dist/api.js ./src/server\"",
    "tsc": "tsc --pretty",
    "lint": "eslint .",
    "copy:better_sqlite3.node": "copyfiles -u 2 \"node_modules/better-sqlite3/build/Release/better_sqlite3.node\" ./"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^11.6.0",
    "bindings": "^1.5.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/css-modules": "^1.0.5",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.6",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "concurrently": "^9.0.1",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pages": "^0.32.3",
    "wait-on": "^8.0.1"
  }
}
