{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Node",
      "skipFiles": ["<node_internals>/**"],
      "port": 9229
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "urlFilter": "http://localhost:5100/*",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
